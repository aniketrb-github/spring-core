<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   
   <bean id="javaCollection" class="org.javabrains.collections.JavaCollections">
   		<property name="addressList">
   			<list>
   				<value>India</value>
   				<value>USA</value>
   				<value>France</value>
   				<value>France</value>
   				<value>Germany</value>
   			</list>
   		</property>
   		
   		<property name="addressSet">
   			<set>
   				<value>India</value>
   				<value>USA</value>
   				<value>France</value>
   				<value>France</value>
   				<value>Germany</value>
   				<value>Germany</value>
   			</set>
   		</property>
   		
   		<property name="addressMap">
   			<map>
   				<entry key="1" value="Greenland"/>
   				<entry key="2" value="U.S.A"/>
   				<entry key="3" value="U.K"/>
   				<entry key="4" value="Indonesia"/>
   				<entry key="5" value="China"/>
   			</map>
   		</property>
   		
   		<property name="addressProp">
   			<props>
   				<prop key="one">Ravi</prop>
   				<prop key="two">Shashi</prop>
   				<prop key="three">Ramdas</prop>
   				<prop key="four">Savita</prop>
   				<prop key="five">Shraddha</prop>
   			</props>
   		</property>
   </bean>
   
   </beans>